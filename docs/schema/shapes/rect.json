{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Rect",
    "description": "A simple rectangle shape",
    "allOf": [
        {
            "$ref": "#/shapes/shape"
        },
        {
            "type": "object",
            "properties": {
                "ty": {
                    "title": "Type",
                    "type": "string",
                    "const": "rc"
                },
                "p": {
                    "title": "Position",
                    "$ref": "#/properties/multi-dimensional"
                },
                "s": {
                    "title": "Size",
                    "$ref": "#/properties/multi-dimensional"
                },
                "r": {
                    "title": "Rounded",
                    "$ref": "#/properties/value"
                }
            },
            "required": [
                "ty"
            ]
        }
    ]
}