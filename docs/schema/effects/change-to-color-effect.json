{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Change To Color Effect",
    "description": "Change to color",
    "allOf": [
        {
            "$ref": "#/$defs/effects/effect"
        },
        {
            "type": "object",
            "properties": {
                "ty": {
                    "title": "Type",
                    "type": "integer",
                    "const": 5
                },
                "ef": {
                    "title": "Effect values",
                    "type": "array",
                    "prefixItems": [
                        {
                            "title": "from_color",
                            "$ref": "#/$defs/effect-values/color"
                        },
                        {
                            "title": "to_color",
                            "$ref": "#/$defs/effect-values/color"
                        },
                        {
                            "title": "change",
                            "$ref": "#/$defs/effect-values/drop-down"
                        },
                        {
                            "title": "change_by",
                            "$ref": "#/$defs/effect-values/drop-down"
                        },
                        {
                            "title": "tolerance",
                            "$ref": "#/$defs/effect-values/ignored"
                        },
                        {
                            "title": "hue",
                            "$ref": "#/$defs/effect-values/slider"
                        },
                        {
                            "title": "lightness",
                            "$ref": "#/$defs/effect-values/slider"
                        },
                        {
                            "title": "saturation",
                            "$ref": "#/$defs/effect-values/slider"
                        },
                        {
                            "title": "saturation_",
                            "$ref": "#/$defs/effect-values/ignored"
                        },
                        {
                            "title": "softness",
                            "$ref": "#/$defs/effect-values/slider"
                        },
                        {
                            "title": "view_correction",
                            "$ref": "#/$defs/effect-values/drop-down"
                        }
                    ]
                }
            },
            "required": [
                "ty"
            ]
        }
    ]
}
