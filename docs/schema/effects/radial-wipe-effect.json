{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Radial Wipe",
    "allOf": [
        {
            "$ref": "#/$defs/effects/effect"
        },
        {
            "type": "object",
            "properties": {
                "ty": {
                    "title": "Type",
                    "type": "integer",
                    "const": 26
                },
                "ef": {
                    "title": "Effect values",
                    "type": "array",
                    "prefixItems": [
                        {
                            "title": "Completion",
                            "description": "Between 0 and 100",
                            "$ref": "#/$defs/effects/effect-value-slider"
                        },
                        {
                            "title": "Start Angle",
                            "$ref": "#/$defs/effects/effect-value-angle"
                        },
                        {
                            "title": "Wipe Center",
                            "$ref": "#/$defs/effects/effect-value-point"
                        },
                        {
                            "title": "Wipe",
                            "$ref": "#/$defs/effects/effect-value-slider"
                        },
                        {
                            "title": "Feather",
                            "$ref": "#/$defs/effects/effect-value-slider"
                        }
                    ]
                }
            },
            "required": [
                "ty"
            ]
        }
    ]
}

